    {
      "path": "dist/bookshelf_wechaty.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.3",
    "@types/mysql": "^2.15.19",
    "@types/nodemailer": "^6.4.4",
    "@types/redis": "^2.8.31",
    "dayjs": "^1.10.6",
    "husky": "^7.0.2",
    "moment": "^2.29.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.0",
    "nodemailer": "^6.6.3",
    "redis": "^3.1.2",
    "sequelize": "^6.6.5",
    "size-limit": "^5.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2",
    "uuid": "^8.3.2"
  }

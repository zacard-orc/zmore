  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@commitlint/utils": "^9.0.1",
    "commitlint-config-gitmoji": "^2.2.3",
    "conventional-changelog-cli": "^2.1.1",
    "conventional-changelog-conventionalcommits": "^4.5.0",
    "image-size": "^1.0.0",
    "nodeman": "^1.1.2",
    "qrcode-terminal": "^0.12.0",
    "wechaty": "^0.60.10",
    "wechaty-puppet-padlocal": "^0.4.1",
    "winston": "^3.3.3"
  },
  "engines": {
    "node": ">=10"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "bookshelf_wechaty",
  "author": "zacard.lin",
  "module": "dist/bookshelf_wechaty.esm.js",
  "size-limit": [
    {
      "path": "dist/bookshelf_wechaty.cjs.production.min.js",
